<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc.mybeziercurveview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.QuadraticActivity"/>
        <activity android:name=".ui.QuadraticShowActivity"/>

        <activity android:name=".ui.CubicActivity"/>
        <activity android:name=".ui.CubicShowActivity"/>

        <activity android:name=".ui.QuarticActivity"/>
    </application>

</manifest>
